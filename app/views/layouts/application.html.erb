<!DOCTYPE html>
<html>
  <head>
    <title>Contact V17</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="UTF-8">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">


  </head>

  <body>
    <%= yield %>
    <%= javascript_pack_tag 'main', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'jquery', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'animsition', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'popper', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'bootstrap', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'select2', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'moment', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'daterangepicker', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'countdowntime', 'data-turbolinks-track': 'reload' %>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-23581568-13');
    </script>
  </body>
</html>
